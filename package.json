{
  "name": "demo-app5",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prettier-format-script": "prettier --config .prettierrc src/**/*.ts --write",
    "prettier-format-template": "prettier --config .prettierrc src/**/*.html --write",
    "prettier-format-style": "prettier --config .prettierrc src/**/*.scss --write",
    "prettier-format-all": "npm run prettier-format-script && npm run prettier-format-template && npm run prettier-format-style",
    "start": "ng serve --configuration development",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint --fix",
    "az-login": "az login",
    "az-upgrade": "az upgrade",
    "az-logout": "az logout",
    "az-account-list": "az account list",
    "az-account-set": "az account set --subscription",
    "az-install-containerapp": "az extension add --name containerapp --upgrade",
    "az-register-stuff": "az provider register --namespace Microsoft.App && az provider register --namespace Microsoft.OperationalInsights",
    "az-create-rg": "az group create --name $RESOURCE_GROUP --location $LOCATION",
    "az-create-acr": "az acr create --resource-group $RESOURCE_GROUP --name $ACR_NAME --sku Basic --admin-enabled true",
    "az-acr-build": "az acr build --registry $ACR_NAME --image $UI_NAME .",
    "az-containerapp-env-create": "az containerapp env create --name $ENVIRONMENT --resource-group $RESOURCE_GROUP --location $LOCATION",
    "az-deploy-ui": "az containerapp create --name $UI_NAME --resource-group $RESOURCE_GROUP --environment $ENVIRONMENT --image $ACR_NAME.azurecr.io/demo5-ui --target-port 3000 --env-vars API_BASE_URL=https://$API_BASE_URL --ingress external --registry-server $ACR_NAME.azurecr.io --query properties.configuration.ingress.fqdn",
    "az-create-aks": "az acr create --resource-group $RESOURCE_GROUP --name $CLUSTER_NAME --node-count 1 --enable-addons monitoring --generate-ssh-keys",
    "openapi-gen-v1": "openapi-generator-cli generate -g typescript-angular -i http://localhost:5071/swagger/v1/swagger.json -o src\\backend-api\\v1 -p npmName=demo-app5-api-v1",
    "nvm": "nvm use 18.13.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/cdk": "^17.1.2",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/material": "^17.1.2",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "eslint-scope": "^8.0.0",
    "file-saver": "^2.0.5",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.7.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.3",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.1.3",
    "@angular/compiler-cli": "^17.1.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.2"
  }
}
